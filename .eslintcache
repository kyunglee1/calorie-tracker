 [props.clickedAdd]);\n\n  const panesList = props.panes.map((pane) => (\n    <CalorieLogPane key={pane.fdcId} id={pane.fdcId} />\n  ));\n\n  const totalCalories = props.panes.reduce(\n    (total, pane) => total + pane.calorieCount,\n    0\n  );\n\n  return (\n    <div className=\"log-container\">\n      <div id=\"log-header\">CALORIE LOG</div>\n      <div className=\"panes\">{panesList}</div>\n      {props.panes.length > 0 && (\n        <div id=\"log-footer\">\n          <span>{`Total: ${totalCalories} kcal`}</span>\n        </div>\n      )}\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => ({\n  results: state.results.searchResults,\n  panes: state.calorieLog.panes,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  addPane: (pane) => dispatch(addPane(pane)),\n  signalDuplicate: (id) => dispatch(signalDuplicate(id)),\n  endSignal: (id) => dispatch(endSignal(id)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(CalorieLog);\n","/Users/kyunglee/Desktop/nutrition-tracker-app/src/actions/fetchData.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/reducers/resultsReducer.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/actions/types.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/reducers/panesReducer.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/actions/signalDuplicate.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/actions/addPane.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/actions/endSignal.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/components/ResultRow/index.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/components/CalorieLogPane/index.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/actions/removePane.js",[],"/Users/kyunglee/Desktop/nutrition-tracker-app/src/actions/updatePane.js",[],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId"